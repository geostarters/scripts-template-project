{
  "name": "scripts-project-template",
  "version": "1.1.0",
  "description": "Template for scripting",
  "main": "index.js",
  "keywords": [
    "node",
    "scripts"
  ],
  "engines": {
    "node": ">=8.x"
  },
  "author": "@geostarters",
  "license": "MIT",
  "scripts": {
    "build": "run-s lint clean flow:build",
    "run:example": "node ./scripts/example.js",
    "clean": "rimraf scripts/*",
    "lint": "eslint --fix --cache --ignore-path .gitignore src",
    "flow": "flow",
    "flow:check": "flow check ./src/*.js",
    "flow:build": "flow-remove-types ./src/ -d ./scripts/ --all --pretty",
    "test": "jest"
  },
  "dependencies": {
    "@geostarters/common": "^0.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-transform-flow-strip-types": "^7.10.4",
    "@geostarters/eslint-config": "^0.0.6",
    "babel-jest": "^26.3.0",
    "envify": "^4.1.0",
    "eslint-config-node": "^4.1.0",
    "eslint-plugin-node": "^11.1.0",
    "flow-bin": "^0.133.0",
    "flow-remove-types": "^2.133.0",
    "flow-typed": "^3.2.1",
    "jest": "^26.4.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  }
}
